{
  "name": "wedding-page",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "lithuanian": "ng serve --aot --i18n-file=src/locale/messages.lt.xlf --locale=lt --i18n-format=xlf --open",
    "polish": "ng serve --aot --i18n-file=src/locale/polish.lt.xlf --locale=pl --i18n-format=xlf --open",
    "test": "ng test",
    "coverage": "ng test --code-coverage --watch=false && start coverage/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng-xi18n -p src/tsconfig.json",
    "build-i18n:en": "ng build --output-path=dist/en --aot -prod --bh /en/ --i18n-file=src/locale/messages.xlf --i18n-format=xlf --locale=en",
    "build-i18n:lt": "ng build --output-path=dist/lt --aot -prod --bh /lt/ --i18n-file=src/locale/messages.lt.xlf --i18n-format=xlf --locale=lt",
    "build-i18n:pl": "ng build --output-path=dist/pl --aot -prod --bh /pl/ --i18n-file=src/locale/messages.pl.xlf --i18n-format=xlf --locale=pl",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:lt && npm run build-i18n:pl"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.7",
    "@angular/core": "^2.4.7",
    "@angular/forms": "^2.4.7",
    "@angular/http": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/router": "^3.4.7",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.7",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0-beta.4",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0"
  }
}
